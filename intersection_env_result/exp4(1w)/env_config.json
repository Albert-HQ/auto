{
    "simulation_frequency": 30,
    "policy_frequency": 1,
    "real_time_rendering": true,
    "controlled_vehicles": 2,
    "duration": 300,
    "observation": {
        "type": "MultiAgentObservation",
        "observation_config": {
            "type": "Kinematics",
            "flatten": true,
            "absolute": true,
            "see_behind": true,
            "normalize": false,
            "features": ["x", "y", "vx", "vy"],
            "vehicles_count": 2
            }
    },
    "action": {
        "type": "MultiAgentAction",
        "action_config": {
            "type": "ContinuousAction",  
            "longitudinal": true,
            "lateral": false
            }
    },
    "arrived_reward": 10,
    "collision_reward": -10,
    "high_speed_reward": 0,
    "on_road_reward": 0,
    "offroad_terminal": true,
    "first_arrive_reward": 20,
    "second_arrive_reward": 10,
    "recycle_on_arrival": true,
    "recycle_if_stuck": true,
    "stuck_speed_threshold": 0.3,
    "stuck_time_seconds": 3.0
}